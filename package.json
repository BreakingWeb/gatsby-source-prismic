{
  "private": true,
  "name": "gatsby-source-prismic",
  "version": "3.0.0-beta.19",
  "description": "Gatsby source plugin for building websites using prismic.io as a data source",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "gatsby-node.js",
    "gatsby-browser.js"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "format": "prettier --write '{{src,docs}/**/*.{ts,tsx,md}}'",
    "prepare": "npm run build",
    "release": "standard-version",
    "test": "jest"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-prismic.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "prismic"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-typescript": "^3.0.0",
    "@types/jest": "^25.1.1",
    "cross-env": "^7.0.0",
    "gatsby": "^2.19.12",
    "husky": "^4.2.1",
    "prettier": "^1.19.1",
    "rollup": "^1.31.0",
    "standard-version": "^7.1.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "gatsby": ">=2.3",
    "react": ">=16.8"
  },
  "browserslist": ">1%, not ie 11, not op_mini all",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "printWidth": 80
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
